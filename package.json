{
  "name": "n8n-nodes-thordata",
  "version": "0.2.1",
  "description": "Minimal n8n node to access Thordata cloud HTTP APIs (search engine, universal scrape, smart scrape).",
  "keywords": [
    "n8n-community-node-package",
    "thordata",
    "web-scraping",
    "search"
  ],
  "license": "MIT",
  "author": {
    "name": "Thordata",
    "email": "support@thordata.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "echo \"No tests configured\""
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "n8n": {
    "n8nNodesApiVersion": 2,
    "credentials": [
      "dist/credentials/ThordataApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Thordata/Thordata.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "n8n-core": "^1.0.0",
    "n8n-workflow": "^1.36.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.7.0"
  }
}